#tempo negra 100
#compas 3/4

const oct1 = 6;
const oct2 = 5;

// Instrumentos
const organo = 18;

voz (organo)
{
	compas
	{
		nota(la, oct1, negra);
		nota(la, oct1, negra);
		nota(sol, oct1, corchea);
		nota(fa, oct1, corchea);
	}

	compas
	{
		nota(mi, oct1, negra.);
		nota(do, oct1, semicorchea);
		nota(re, oct1, semicorchea);
		nota(mi, oct1, corchea);
		nota(do, oct1, corchea);
	}

	compas
	{
		nota(fa, oct2, blanca.);
	}

	compas
	{
		silencio(negra);
		silencio(semicorchea);
		nota(mi, oct2, semicorchea);
		nota(mi, oct2, semicorchea);
		nota(fa, oct2, semicorchea);
	}
}

